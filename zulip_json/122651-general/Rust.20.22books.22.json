    {
        "content": "<ul>\n<li><a href=\"https://rust-lang.github.io/async-book/\">https://rust-lang.github.io/async-book/</a></li>\n</ul>",
        "id": 214680417,
        "sender_full_name": "njn",
        "timestamp": 1603793466
    },
    {
        "content": "<ul>\n<li><a href=\"https://rust-cli.github.io/book/index.html\">https://rust-cli.github.io/book/index.html</a></li>\n</ul>",
